"use strict";function countdown(){var t=new Date,e=new Date(2018,7,14),n=t.getTime(),o=e.getTime()-n,d=Math.floor(o/1e3),m=Math.floor(d/60),u=Math.floor(m/60),c=Math.floor(u/24);u=(u%=24)<10?"0"+u:u,m=(m%=60)<10?"0"+m:m,d=(d%=60)<10?"0"+d:d,document.getElementById("days").textContent=c,document.getElementById("hours").textContent=u,document.getElementById("minutes").textContent=m,document.getElementById("seconds").textContent=d,setTimeout(countdown,1e3)}document.getElementById("days")&&countdown();